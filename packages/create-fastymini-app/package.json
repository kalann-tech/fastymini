{
  "name": "create-fastymini-app",
  "description": "A CLI tool to create a FastyMini app by KALAAN",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kalann-tech/fastymini.git",
    "directory": "packages/create-fastymini-app"
  },
  "author": "Rayan Traore <rayan.traore03@gmail.com>",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/figlet": "^1.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.7.0",
    "@types/prompts": "^2.4.9",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "cli-spinners": "^3.3.0",
    "commander": "^14.0.1",
    "execa": "^9.6.0",
    "figlet": "^1.9.3",
    "fs-extra": "^11.3.2",
    "ora": "^8.2.0",
    "prompts": "^2.4.2",
    "update-check": "^1.5.4"
  },
  "keywords": [
    "create",
    "fastify",
    "typescript",
    "starter"
  ],
  "bin": "./dist/index.js",
  "version": "1.3.0",
  "scripts": {
    "release": "ncc build ./src/index.ts -o ./dist/ --minify --no-cache --no-source-map-register --external figlet",
    "start": "node dist/index.js",
    "dev": "ts-node --project tsconfig.json ./src/index.ts",
    "build": "yarn release",
    "prepublishOnly": "yarn build && git add .",
    "lint": "yarn prettier -w 'templates/**/*.ts' --config ./templates/.prettierrc.json",
    "check-templates": "ts-node scripts/check-templates.ts",
    "sync-templates": "ts-node scripts/sync-templates.ts",
    "rename": "ts-node scripts/rename-config.ts",
    "restore": "ts-node scripts/restore-config.ts",
    "local:link": "yarn build && head -n 1 ./dist/index.js && npm link",
    "local:unlink": "npm unlink create-fastymini-app"
  },
  "files": [
    "dist",
    "templates"
  ],
  "license": "MIT"
}
